{
  "name": "vite-plugin-jsonx",
  "version": "1.1.2",
  "author": "Gokhan Taskan",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/vite-plugin-jsonx.esm.js",
  "files": [
    "dist",
    "src",
    "client.d.ts"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint .",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "jsonc": "^2.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^12.0.0",
    "bunchee": "^6.9.3",
    "husky": "^9.1.7",
    "oxlint": "^1.39.0",
    "size-limit": "^12.0.0",
    "tsdx": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "20 KB",
      "ignore": ["fs", "path", "util", "assert", "stream", "constants"]
    },
    {
      "path": "dist/vite-plugin-jsonx.esm.js",
      "limit": "20 KB",
      "ignore": ["fs", "path", "util", "assert", "stream", "constants"]
    }
  ]
}
