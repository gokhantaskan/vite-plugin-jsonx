{
  "name": "vite-plugin-jsonx",
  "version": "1.2.0",
  "author": "Gokhan Taskan",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/vite-plugin-jsonx.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/vite-plugin-jsonx.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./client": "./client.d.ts"
  },
  "files": [
    "dist",
    "src",
    "client.d.ts"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "node esbuild.config.mjs && tsc --emitDeclarationOnly --declaration --outDir dist",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "jsonc": "^2.0.0"
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^12.0.0",
    "@types/node": "^20.19.29",
    "@vitest/coverage-v8": "^4.0.17",
    "esbuild": "^0.27.2",
    "husky": "^9.1.7",
    "oxlint": "^1.39.0",
    "size-limit": "^12.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "oxlint"
    }
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "20 KB",
      "ignore": [
        "fs",
        "path",
        "util",
        "assert",
        "stream",
        "constants"
      ]
    },
    {
      "path": "dist/vite-plugin-jsonx.esm.js",
      "limit": "20 KB",
      "ignore": [
        "fs",
        "path",
        "util",
        "assert",
        "stream",
        "constants"
      ]
    }
  ]
}

